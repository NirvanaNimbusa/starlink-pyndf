*   INTEGER*8 arithmetic functions.
      INTEGER*8 NUM_ADDK
      INTEGER*8 NUM_SUBK
      INTEGER*8 NUM_MULK
      INTEGER*8 NUM_DIVK
      INTEGER*8 NUM_IDVK
      INTEGER*8 NUM_PWRK
      INTEGER*8 NUM_NEGK


*   INTEGER*8 functions with a single INTEGER*8 argument.
      INTEGER*8 NUM_SQRTK
      INTEGER*8 NUM_LOGK
      INTEGER*8 NUM_LG10K
      INTEGER*8 NUM_EXPK
      INTEGER*8 NUM_ABSK
      INTEGER*8 NUM_NINTK
      INTEGER*8 NUM_INTK


*   INTEGER*8 functions with two INTEGER*8 arguments.
      INTEGER*8 NUM_MINK
      INTEGER*8 NUM_MAXK
      INTEGER*8 NUM_DIMK
      INTEGER*8 NUM_MODK
      INTEGER*8 NUM_SIGNK


*   Logical functions comparing two INTEGER*8 numbers.
      LOGICAL NUM_EQK
      LOGICAL NUM_NEK
      LOGICAL NUM_GTK
      LOGICAL NUM_GEK
      LOGICAL NUM_LTK
      LOGICAL NUM_LEK



